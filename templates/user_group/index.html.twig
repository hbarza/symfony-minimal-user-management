{% extends 'base.html.twig' %}

{% block title %}UserGroup index{% endblock %}

{% block body %}
    <h1>UserGroup index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Create_at</th>
                <th>Update_at</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_group in user_groups %}
            <tr>
                <td>{{ user_group.id }}</td>
                <td>{{ user_group.name }}</td>
                <td>{{ user_group.createAt ? user_group.createAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ user_group.updateAt ? user_group.updateAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('user_group_show', {'id': user_group.id}) }}">show</a>
                    <a href="{{ path('user_group_edit', {'id': user_group.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('user_group_new') }}">Create new</a>
{% endblock %}
